{
    "name": "ggggino/skuskucart-bundle",
    "description": "This bundle aim to be a slim cart manager",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "David Ginanni",
            "email": "david.ginanni@gmail.com"
        }
    ],
    "keywords": ["cart", "bundle", "symfony", "symfony3", "symfony4"],
    "homepage": "https://github.com/GGGGino",
    "require": {
        "php": "^5.6|^7.0",
        "craue/formflow-bundle": "^3.0",
        "knplabs/knp-snappy-bundle": "^1.4",
        "liuggio/excelbundle": "^2.1",
        "symfony/http-kernel": "^3.3|^4.0",
        "payum/payum-bundle": "^2.3"
    },
    "require-dev": {
        "symfony/templating": "^3.3|^4.0",
        "symfony/twig-bundle": "^3.3|^4.0",
        "symfony/asset": "^3.3|^4.0",
        "symfony/console": "^3.3|^4.0",
        "symfony/config": "^3.3|^4.0",
        "symfony/validator": "^3.3|^4.0",
        "symfony/property-access": "^3.3|^4.0",
        "symfony/form": "^3.3|^4.0",
        "symfony/dom-crawler": "^3.3|^4.0",
        "symfony/browser-kit": "^3.3|^4.0",
        "symfony/cache": "^3.3|^4.0",
        "symfony/phpunit-bridge": "^3.3|^4.0",
        "symfony/stopwatch": "^3.3|^4.0",
        "sensio/framework-extra-bundle": "^3.0",
        "doctrine/annotations": "^1.2",
        "doctrine/common": "^2.4"
    },
    "suggest": {
        "payum/payum-bundle": "Allow to automatize the payment process"
    },
    "autoload": {
        "psr-4": {
            "GGGGino\\SkuskuCartBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
